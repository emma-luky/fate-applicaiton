{
  "name": "hello-rn",
  "version": "0.0.0",
  "main": "src/index.ts",
  "scripts": {
    "start": "expo start",
    "db:reset": "tsx src/scripts/reset.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "clean": "rm -rf .expo dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/config-plugins": "^8.0.8",
    "@floating-ui/react-dom": "^2.1.1",
    "@floating-ui/react-native": "^0.10.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@tamagui/config": "^1.98.3",
    "@tamagui/font-inter": "^1.98.3",
    "@tamagui/lucide-icons": "^1.98.3",
    "@unimodules/core": "^7.2.0",
    "expo": "^51.0.14",
    "expo-app-loading": "^1.0.3",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.20",
    "expo-font": "~12.0.5",
    "expo-image-picker": "^15.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.18",
    "expo-splash-screen": "^0.27.5",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.12.4",
    "montserrat": "^1.0.1",
    "playwright": "^1.45.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.74.2",
    "react-native-fs": "^2.20.0",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "tamagui": "^1.98.3",
    "expo-file-system": "~17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.2.5",
    "react-native-svg-transformer": "^1.4.0",
    "tsx": "^4.15.5",
    "typescript": "^5.5.4"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "app.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ],
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "importOrder": [
      "^react$",
      "^react-native$",
      "<THIRD_PARTY_MODULES>",
      "",
      "^[./]"
    ]
  },
  "private": true
}
